{
    "name": "boost/craft-env-admin",
    "description": "Quickly and easily manage your .env variables from the Craft CMS control panel",
    "type": "craft-plugin",
    "version": "dev-master",
    "license": "proprietary",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "ENV"
    ],
    "support": {
        "email": "matthewdejager5@gmail.com",
        "issues": "https://github.com/git@github.com:D3VM4TT/craft-env-admin.git/issues?state=open",
        "source": "https://github.com/git@github.com:D3VM4TT/craft-env-admin.git",
        "docs": "https://github.com/git@github.com:D3VM4TT/craft-env-admin.git",
        "rss": "https://github.com/git@github.com:D3VM4TT/craft-env-admin.git/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.0.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "boost\\envadmin\\": "src/"
        }
    },
    "extra": {
        "handle": "env-admin",
        "name": "Env Admin",
        "developer": "Boost",
        "documentationUrl": "https://github.com/git@github.com:D3VM4TT/craft-env-admin.git"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

